{
  "manifest_version": 2,

  "name": "astrologarble",
  "description": "This extension swipe words in susan miller page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
    }]
}
